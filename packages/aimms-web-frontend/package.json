{
  "private": "true",
  "type": "module",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --reporter=verbose || true",
    "test:bundle": "npm run build && vitest run src/__tests__/bundle-size.test.ts",
    "test:performance": "npm run test:bundle && npm run analyze:bundle",
    "test:e2e": "playwright test --config=e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=e2e/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=e2e/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report e2e/playwright-report",
    "test:e2e:trace": "PLAYWRIGHT_TRACE=on PLAYWRIGHT_VIDEO=on playwright test --config=e2e/playwright.config.ts",
    "analyze:bundle": "npm run build && open dist/stats.html",
    "check:sizes": "npm run build && ls -lh dist/assets/*.js && echo '\nâœ… Compressed sizes:' && ls -lh dist/assets/*.js.gz",
    "storybook": "storybook dev -p 6006 --config-dir ../../.storybook",
    "build-storybook": "storybook build --config-dir ../../.storybook",
    "chromatic": "npm run build-storybook && dotenv -e ../../.env -- chromatic --exit-once-uploaded --config-file=config/chromatic.config.json",
    "chromatic:prebuild": "dotenv -e ../../.env -- chromatic --exit-once-uploaded --config-file=config/chromatic.config.json"
  },
  "dependencies": {
    "@date-io/dayjs": "^2.17.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google-cloud/storage": "^7.15.2",
    "@mui/icons-material": "^6.4.12",
    "@mui/material": "^6.4.12",
    "@mui/x-data-grid": "^7.22.2",
    "@mui/x-date-pickers": "^7.22.2",
    "@tanstack/react-query-devtools": "^5.49.2",
    "@types/react-query": "^1.2.8",
    "axios": "^1.8.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^12.9.2",
    "jose": "^6.0.8",
    "jwt-decode": "^4.0.0",
    "material-react-table": "^3.2.1",
    "openai": "^4.77.0",
    "query-string": "^9.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-dropzone": "^14.3.8",
    "react-is": "^18.3.1",
    "react-joyride": "^2.9.3",
    "react-query": "^3.39.3",
    "react-router-dom": "6.20.0",
    "recharts": "^2.15.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.0.4",
    "@storybook/addon-docs": "^10.0.4",
    "@storybook/addon-links": "^10.0.4",
    "@storybook/addon-themes": "^10.0.4",
    "@storybook/react-vite": "^10.0.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.10.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/recharts": "^1.8.29",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "chromatic": "^13.3.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "10.0.4",
    "jsdom": "^23.1.0",
    "msw": "^2.11.6",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^10.0.4",
    "typescript": "^5.1.6",
    "vite": "^6.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.0.7"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.19.0",
    "@rollup/rollup-linux-x64-gnu": "^4.9.5"
  },
  "lint-staged": {
    "frontend/src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "bracketSpacing": true,
    "bracketSameLine": true,
    "printWidth": 100,
    "jsxSingleQuote": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.tsx",
        "options": {
          "parser": "typescript"
        }
      }
    ]
  },
  "name": "frontend"
}
